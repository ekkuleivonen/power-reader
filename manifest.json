{
    "manifest_version": 3,
    "name": "Power Reader",
    "description": "Read web content 2x as fast.",
    "version": "1.0",
    "icons": {
        "16": "/images/power-reader-icon-16px.jpg",
        "48": "/images/power-reader-icon-48px.jpg",
        "128": "/images/power-reader-icon-128px.jpg"
    },
    "page_action": {
        "default_icon": {
            "16": "/images/power-reader-icon-16px.jpg",
            "48": "/images/power-reader-icon-48px.jpg",
            "128": "/images/power-reader-icon-128px.jpg"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["/dist/main.js"]
        }
    ]
}
